<%- |
  String $id,
  Enum['ipv4', 'ipv6'] $ip_version,
  String $import,
  String $export,
  Enum['v2', 'v3'] $ospf_version,
  String $table,
| -%>
protocol ospf <%= $ospf_version %> <%= $id %> {
  <%= $ip_version %> {
    import <%= $import %>;
    export <%= $export %>;
    table <%= $table %>;
  };

