<%- |
  String $export,
  String $import,
  Enum['ipv4', 'ipv6'] $protocol,
  String $table = 'master4',
  Integer $kernel_table = 254,
| -%>
# This file is managed by puppet
# !! DO NOT EDIT MANUALLY  !!

protocol kernel {
  <%= $protocol %> {
    export <%= $export %>;
    import <%= $import %>;
    table <%= $table %>;
  };

  kernel table <%= $kernel_table %>;
}
