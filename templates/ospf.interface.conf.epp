<%- |
  String $iface,
  Boolean $stub,
  Array[String] $neighbours,
| -%>
    interface "<%= $iface %>" {
      <%- if $stub { -%>
      stub;
      <%- } -%>
      <% unless $peers.empty { %>
      neighbors {
        <% $neighbours.each |$neigh| { %>
        <%= $neigh %>,
        <% } %>
      };
      <% } %>
    };

